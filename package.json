{
  "name": "serial-io",
  "version": "0.1.3",
  "description": "serialport interface for batch style commands",
  "license": "MIT",
  "repository": "anoff/serial-io",
  "author": {
    "name": "anoff",
    "email": "offenhaeuser@gmail.com",
    "url": "github.com/anoff"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "xo && nyc ava",
    "test:watch": "ava --watch",
    "test:coverage": "nyc ava"
  },
  "keywords": [
    "serial",
    "io",
    "string",
    "commands"
  ],
  "dependencies": {
    "serialport": "^6.0.5"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "coveralls": "^3.0.0",
    "nyc": "^11.0.2",
    "testdouble": "^3.0.0",
    "xo": "^0.20.3"
  },
  "xo": {
    "esnext": true
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "ava": {
    "files": [
      "!test/*.js",
      "**/*.test.js"
    ]
  }
}
